<Application x:Class="DMOrganizerApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:DMOrganizerApp"
             xmlns:conv="clr-namespace:WPFToolbox.Converter;assembly=WPFToolbox"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <BitmapImage x:Key="CollapsedCategoryIcon" UriSource="/DMOrganizerApp;component/Resources/CollapsedCategoryIcon.png" />
        <BitmapImage x:Key="ExpandedCategoryIcon" UriSource="/DMOrganizerApp;component/Resources/ExpandedCategoryIcon.png" />
        <BitmapImage x:Key="DocumentIcon" UriSource="/DMOrganizerApp;component/Resources/DocumentIcon.png" />
        <BitmapImage x:Key="ClosedBookIcon" UriSource="/DMOrganizerApp;component/Resources/ClosedBookIcon.png" />
        <BitmapImage x:Key="OpenedBookIcon" UriSource="/DMOrganizerApp;component/Resources/OpenedBookIcon.png" />
        <BitmapImage x:Key="PageIcon" UriSource="/DMOrganizerApp;component/Resources/Page.png" />
        <conv:NullToLoading x:Key="NullToLoading" />
    </Application.Resources>
</Application>
